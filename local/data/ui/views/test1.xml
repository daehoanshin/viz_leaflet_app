<dashboard>
  <label>현대자동차 테스트</label>
  <row>
    <panel>
      <viz id="mapid" type="viz_leaflet_app.leaflet">
        <search>
          <query>| inputlookup makers.csv</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="viz_leaflet_app.leaflet.bindId">test</option>
        <option name="drilldown">none</option>
        <option name="height">681</option>
        <option name="viz_leaflet_app.leaflet.mainColor">#f7bc38</option>
        <option name="viz_leaflet_app.leaflet.maxValue">100</option>
      </viz>
    </panel>
  </row>
</dashboard>